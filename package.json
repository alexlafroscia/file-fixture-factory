{
  "name": "file-fixture-factory",
  "version": "3.1.0",
  "description": "Declaratively create real file system fixtures",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "test": "vitest",
    "postinstall": "husky install",
    "prepublishOnly": "yarn build && pinst --disable",
    "postpublish": "pinst --enable",
    "release": "standard-version"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "url": "git@github.com:alexlafroscia/file-fixture-factory.git",
    "type": "git"
  },
  "author": "Alex LaFroscia <alex@lafroscia.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.1"
  },
  "devDependencies": {
    "@movable/eslint-config": "^0.6.9",
    "@movable/eslint-config-node": "^1.0.0",
    "@movable/eslint-config-typescript": "^1.0.1",
    "@movable/prettier-config": "^0.3.6",
    "@tsconfig/node14": "^1.0.0",
    "@types/debug": "^4.1.5",
    "@types/node": "^20.2.5",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "pinst": "^2.1.4",
    "standard-version": "^9.1.0",
    "typescript": "^4.1.5",
    "vitest": "^0.29.1"
  },
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.10"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{md,json,yml}": "prettier --write"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  }
}
